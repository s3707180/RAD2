<%= form_with(model: attempt) do |form| %>
  <% if attempt.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(attempt.errors.count, "error") %> prohibited this attempt from being saved:</h2>

      <ul>
        <% attempt.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :dateandtime %>
    <%= form.datetime_select :dateandtime %>
  </div>

  <div class="field">
    <%= form.label :category %>
    <%= form.text_field :category %>
  </div>

  <div class="field">
    <%= form.label :difficulty %>
    <%= form.text_field :difficulty %>
  </div>

  <div class="field">
    <%= form.label :questionsList %>
    <%= form.text_field :questionsList %>
  </div>

  <div class="field">
    <%= form.label :questionnum %>
    <%= form.number_field :questionnum %>
  </div>

  <div class="field">
    <%= form.label :questioncorrect %>
    <%= form.number_field :questioncorrect %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
