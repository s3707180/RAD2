<h1>Quiz Result</h1>

<H4 id="notice"><%= notice %></H4>

<div class="quiz">

    <h3>
      This time, <%= @message %>
    </h3>
    <hr>
    
    <% for result in @preResults.reverse do %>
      <%= result %> <br>
    <% end %>
    <br>

    <%= form_with url: "/attempts/start", method: :get do |form| %>
      <%= form.submit "Reload", style:'height:40px;width:100%;' %>
      <br>
      <br>
      <br>
      <%= form.submit "Redo", style:'height:40px;width:100%;' %>
    <% end %>

</div>