<p id="notice"><%= notice %></p>

<h1>Attempts</h1>

<table>
  <thead>
    <tr>
      <th>Dateandtime</th>
      <th>Category</th>
      <th>Difficulty</th>
      <th>Questionslist</th>
      <th>Questionnum</th>
      <th>Questioncorrect</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @attempts.each do |attempt| %>
      <tr>
        <td><%= attempt.dateandtime %></td>
        <td><%= attempt.category %></td>
        <td><%= attempt.difficulty %></td>
        <td><%= attempt.questionsList %></td>
        <td><%= attempt.questionnum %></td>
        <td><%= attempt.questioncorrect %></td>
        <td><%= link_to 'Show', attempt %></td>
        <td><%= link_to 'Edit', edit_attempt_path(attempt) %></td>
        <td><%= link_to 'Destroy', attempt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Attempt', new_attempt_path %>
